{
  "name": "rspec-report-action",
  "version": "6.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "npx ncc build src/main.ts -o dist --source-map --license licenses.txt",
    "test": "jest",
    "all": "pnpm run build && pnpm run format && pnpm run lint && pnpm run package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SonicGarden/rspec-report-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "author": "aki77",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@aki77/actions-replace-comment": "0.6.1",
    "fast-glob": "^3.3.2",
    "markdown-table": "^3.0.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vercel/ncc": "^0.38.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^5.0.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.3.2",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3"
  },
  "volta": {
    "node": "20.15.0",
    "yarn": "1.22.19"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
